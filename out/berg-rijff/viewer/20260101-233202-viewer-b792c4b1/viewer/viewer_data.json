    {
      "id": "text/62",
      "kind": "text",
      "page_no": 4,
      "label": "text",
      "text": "Bericht van Rijff Juridisch",
      "area": 3838.2222222222736,
      "x": 199.33333333333334,
      "y": 528.6666666666665,
      "w": 169.33333333333334,
